"import type { Construct, TerraformProviderConfig, TfString, TfNumber, TfBoolean, TfStringList, TfNumberList, TfStringMap } from "tfts";
import { TerraformProvider } from "tfts";

export type SimpleProviderConfig = {
  readonly apiKey: TfString;
  readonly region?: TfString;
} & TerraformProviderConfig;

export class SimpleProvider extends TerraformProvider {
  constructor(scope: Construct, id: string, config: SimpleProviderConfig = {}) {
    super(scope, id, "registry.terraform.io/hashicorp/simple", {
      api_key: config.apiKey,
      region: config.region,
    }, config);
  }
}

export type ResourceConfigConfig = {
  readonly key: TfString;
  readonly value?: TfString;
};
export type ResourceConfigItems = {
  readonly name: TfString;
};
export type ResourceConfig = {
  readonly id?: TfString;
  readonly name: TfString;
  readonly enabled?: TfBoolean;
  readonly count?: TfNumber;
  readonly tags?: TfStringMap;
  readonly config?: ResourceConfigConfig;
  readonly items: readonly ResourceConfigItems[];
} & TerraformResourceConfig;
export class Resource extends TerraformResource {
  constructor(scope: Construct, id: string, config: ResourceConfig) {
    super(scope, id, "simple_resource", {
      id: config.id,
      name: config.name,
      enabled: config.enabled,
      count: config.count,
      tags: config.tags,
      config: config.config,
      items: config.items,
    }, config);
  }
  get id(): TokenString {
    return this.getStringAttribute("id");
  }
}

export type DataDataConfig = {
  readonly id: TfString;
  readonly name?: TfString;
} & TerraformDataSourceConfig;
export class DataData extends TerraformDataSource {
  constructor(scope: Construct, id: string, config: DataDataConfig) {
    super(scope, id, "simple_data", {
