{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "tfts",
  "colors": {
    "primary": "#7C3AED",
    "light": "#A78BFA",
    "dark": "#5B21B6"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
