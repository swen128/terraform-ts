          {
            "group": "Resources",
            "pages": [
              "api-reference/resource",
              "api-reference/data-source",
              "api-reference/provider"
            ]
          },
          {
            "group": "Declarations",
            "pages": ["api-reference/variable", "api-reference/output", "api-reference/local"]
          },
          {
            "group": "Backends",
            "pages": ["api-reference/backends"]
          },
          {
            "group": "Functions",
            "pages": ["api-reference/functions", "api-reference/operators"]
          },
          {
            "group": "Advanced",
            "pages": ["api-reference/iterator", "api-reference/remote-state"]
          }
        ]
      },
      {
        "tab": "Testing",
        "groups": [
          {
            "group": "Testing",
            "pages": ["testing/overview"]
          }
        ]
      },
