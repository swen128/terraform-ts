          },
          {
            "group": "Core Concepts",
            "pages": ["guides/core-concepts", "guides/references", "guides/variables-outputs"]
          },
          {
            "group": "Infrastructure",
            "pages": ["guides/functions", "guides/backends"]
          },
          {
            "group": "Advanced",
            "pages": [
              "advanced/aspects",
              "advanced/iterators",
              "advanced/remote-state",
              "advanced/assets",
              "advanced/modules",
              "advanced/tokens"
            ]
          },
          {
            "group": "Migration",
            "pages": ["guides/migration-cdktf"]
          }
        ]
      },
      {
        "tab": "CLI Reference",
        "groups": [
          {
            "group": "Commands",
            "pages": [
              "cli-reference/overview",
              "cli-reference/get",
              "cli-reference/synth",
              "cli-reference/diff",
              "cli-reference/deploy",
              "cli-reference/destroy",
