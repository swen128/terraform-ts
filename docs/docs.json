    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/swen128/terraform-ts",
          "icon": "github"
        },
        {
          "anchor": "npm",
          "href": "https://www.npmjs.com/package/tfts",
          "icon": "npm"
        }
      ]
    }
