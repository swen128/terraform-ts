{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "workspaces": {
    ".": {
      "entry": ["bin/tfts.ts"],
      "ignore": ["**/*.spec.ts", "**/*.test.ts"]
    },
    "e2e-tests/*": {
      "entry": ["main.ts"]
    }
  },
  "ignoreExportsUsedInFile": true,
  "eslint": {
    "config": ["eslint.config.js"]
  },
  "typescript": {
    "config": ["tsconfig.json"]
  }
}
